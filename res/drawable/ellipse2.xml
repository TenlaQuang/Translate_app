<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Định nghĩa màu nền -->
    <solid android:color="#41E341EB" />
    <!-- Định nghĩa kích thước của ellipse -->
    <size
        android:width="100dp"
        android:height="100dp" />

    <!-- Định nghĩa các góc bo tròn (mặc dù không cần thiết cho ellipse, nhưng có thể dùng để làm bo tròn nếu cần) -->
    <corners android:radius="150dp" />
</shape>